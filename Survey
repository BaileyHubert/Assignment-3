package main;
import java.util.Random;
public class Survey {
	
//* creates new random survey number 1-1000000
Random random = new Random();
private int surveyNumber = random.nextInt(1000000);

//* imports question set from text file
private String questionSet = questionSet.txt;

//*creates answer set variable
private String answerSet;

//* prints out questions to be answered
public String questionSet() {
System.out.println(questionSet);
}

//* getter and setter to record answers
public String getanswerSet() {
	return answerSet;
}
public void setLastName(String answerSet) {
	this.answerSet = answerSet;
}
}
